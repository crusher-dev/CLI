{
  "name": "crusher-cli",
  "version": "2.0.0",
  "author": "Himanshu @himanshu-dixit",
  "repository": "crusherdev/CLI",
  "bin": {
    "crusher-cli": "./bin/run"
  },
  "bugs": "https://github.com/himanshu-dixit/crusher/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1",
    "@oclif/core": "^1.1.1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3",
    "@types/inquirer": "^8.1.3",
    "axios": "^0.24.0",
    "cli-ux": "^6.0.7",
    "command-exists": "^1.2.9",
    "extract-zip": "^2.0.1",
    "fastify": "^3.25.3",
    "husky": "^4.3.0",
    "inquirer": "^8.2.0",
    "lint-staged": "^10.3.0",
    "localtunnel": "^2.0.0",
    "node-fetch": "^2.6.1",
    "open": "^8.4.0",
    "pkg": "^4.4.8",
    "tslib": "^1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4",
    "@types/command-exists": "^1.2.0",
    "@types/localtunnel": "^2.0.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4",
    "eslint": "^7.8.1",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "2.0.5",
    "ts-node": "^8",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/crusherdev/CLI",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "crusher",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b  && oclif-dev manifest && oclif-dev readme",
    "createTarBall": "NODE_ENV=production oclif-dev pack",
    "npxPublish": "NODE_ENV=production oclif-dev pack",
    "createInstallers": "NODE_ENV=production oclif-dev pack:win &&  oclif-dev pack:macos && oclif-dev pack:deb",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "eslint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "createBinary": " pkg . && mkdir binaries && mv crusher-cli-linux binaries/crusher-cli-linux && mv crusher-cli-macos binaries/crusher-cli-macos && mv crusher-cli-win.exe binaries/crusher-cli-win.exe",
    "runLint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "runLintFix": "yarn eslint --fix . --ext .js,.jsx,.ts,.tsx"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  }
}
